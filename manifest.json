{
  "manifest_version": 2,

  "name": "Passwger",
  "description": "Passwger chrome Extension",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["jquery-1.11.2.min.js"],
    "matches": ["http://*/*", "https://*/*"]
    }],
    "permissions": [
      "activeTab",
      "http://localhost:12358/*"
    ]
  }
